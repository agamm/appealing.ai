# Porkbun API Configuration
PORKBUN_API_KEY=your_api_key_here
PORKBUN_SECRET_KEY=your_secret_key_here

# Upstash Redis Configuration for Rate Limiting
# Get these from your Upstash dashboard: https://console.upstash.com/
# The REST URL and token are shown in the database details page
UPSTASH_REDIS_REST_URL=https://YOUR_ENDPOINT.upstash.io
UPSTASH_REDIS_REST_TOKEN=YOUR_REST_TOKEN